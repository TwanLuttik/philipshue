{
  "name": "philipshue",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "repository": "git@github.com:TwanLuttik/philipshue.git",
  "author": "Twan Luttik <twanluttik@gmail.com>",
  "license": "MIT",
  "scripts": {
    "web": "cd ./philipshue.web && vue-cli-service serve",
    "web build": "cd ./philipshue.web && vue-cli-service build",
    "core watch": "cd ./philipshue.core && tsc-watch",
    "core build": "cd ./philipshue.core && uglifyjs lib/index.js -c",
    "prep": "cd philipshue.core && yarn && cd .. && cd philipshue.web && yarn"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "workspaces": [
    "philipshue.core/*",
    "philipshue.web/*",
    "philipshue.logic/*"
  ]
}
